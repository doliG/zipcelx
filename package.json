{
  "name": "zipcelx-es5",
  "version": "1.6.0",
  "description": "JSON Transformer to create .xlsx files. Forked to be ES5 tranpiled and CJS shippable.",
  "main": "lib/module.js",
  "browser": "lib/module.js",
  "standalone-build": "lib/standalone.js",
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watch",
    "lint": "eslint . ./",
    "ci": "yarn run test && yarn run lint",
    "build": "yarn run ci && rimraf lib/* && rollup -c"
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".js": "./node_modules/babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doliG/zipcelx-es5"
  },
  "keywords": [
    "JSON",
    "Excel",
    "xlsx"
  ],
  "author": "Lars Krieger / Guillaume LODI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/doliG/zipcelx-es5/issues"
  },
  "homepage": "https://github.com/doliG/zipcelx-es5#readme",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "eslint": "^3.16.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "jest": "^22.4.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.56.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^8.4.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "dependencies": {
    "file-saver": "^1.3.3",
    "jszip": "^3.1.3",
    "lodash.escape": "^4.0.1"
  }
}
